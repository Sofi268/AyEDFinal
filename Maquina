template <class T> class Maquina{
private:
    int idMaquina;
    bool maqTerminal;

public:
    Maquina(void);
    Maquina(int id, bool terminal); 
    T* enviar(T* dato){};
    void recibir(T* dato){};
};

template <class T> Maquina<T>::Maquina(int id, bool terminal){
    idMaquina = id;
    maqTerminal = terminal;
}
